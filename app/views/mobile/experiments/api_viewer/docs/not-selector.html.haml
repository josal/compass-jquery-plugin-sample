%div{"data-role" => "page"}
  %div{"data-role" => "header"}
    %h1 :not() Selector
  #not1{"data-role" => "content", "data-theme" => "c"}
    %h2.jq-clearfix.roundTop.section-title
      %span.name not
      selector
    .entry-details
      %h4.name
        %span.versionAdded>
          version added:
          %a{:href => "/version/1.0/"} 1.0
        jQuery(':not(selector)')
      %ul.signatures
        %li
          %dl.arguments
            %dt selector
            %dd A selector with which to filter by.
      %p.desc
        %strong> Description:
        Selects all elements that do not match the given selector.
      .longdesc
        %p
          All selectors are accepted inside
          = succeed "," do
            %code :not()
          for example:
          %code :not(div a)
          and
          = succeed "." do
            %code :not(div,a)
      %h3 Example:
      #entry-examples.entry-examples
        #example-0
          %h4
            %span.desc Finds all inputs that are not checked and highlights the next sibling span.  Notice there is no change when clicking the checkboxes since no click events have been linked.
          %pre
            %code.example.demo-code
              :preserve
                &lt;!DOCTYPE html&gt;
                &lt;html&gt;
                &lt;head&gt;
                  &lt;script src="http://code.jquery.com/jquery-latest.js"&gt;&lt;/script&gt;
                &lt;/head&gt;
                &lt;body&gt;
                	&lt;div&gt;
                  &lt;input type="checkbox" name="a" /&gt;
                  &lt;span&gt;Mary&lt;/span&gt;
                &lt;/div&gt;
                
                &lt;div&gt;
                  &lt;input type="checkbox" name="b" /&gt;
                  &lt;span&gt;lcm&lt;/span&gt;
                
                &lt;/div&gt;
                &lt;div&gt;
                  &lt;input type="checkbox" name="c" checked="checked" /&gt;
                
                  &lt;span&gt;Peter&lt;/span&gt;
                &lt;/div&gt;
                &lt;script&gt;
                  $("input:not(:checked) + span").css("background-color", "yellow");
                  $("input").attr("disabled", "disabled");
                
                &lt;/script&gt;
                &lt;/body&gt;
                &lt;/html&gt;          %h4 Demo:
          .demo.code-demo
