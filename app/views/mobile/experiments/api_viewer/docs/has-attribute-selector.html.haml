%div{"data-role" => "page"}
  %div{"data-role" => "header"}
    %h1 Has Attribute Selector [name]
  #attributeHas1{"data-role" => "content", "data-theme" => "c"}
    %h2.jq-clearfix.roundTop.section-title
      %span.name attributeHas
      selector
    .entry-details
      %h4.name
        %span.versionAdded>
          version added:
          %a{:href => "/version/1.0/"} 1.0
        jQuery('[attribute]')
      %ul.signatures
        %li
          %dl.arguments
            %dt attribute
            %dd An attribute name.
      %p.desc
        %strong> Description:
        Selects elements that have the specified attribute, with any value.
      %h3 Example:
      #entry-examples.entry-examples
        #example-0
          %h4
            %span.desc Bind a single click that adds the div id to its text.
          %pre
            %code.example.demo-code
              :preserve
                &lt;!DOCTYPE html&gt;
                &lt;html&gt;
                &lt;head&gt;
                  &lt;script src="http://code.jquery.com/jquery-latest.js"&gt;&lt;/script&gt;
                &lt;/head&gt;
                &lt;body&gt;
                	&lt;div&gt;no id&lt;/div&gt;
                  &lt;div id="hey"&gt;with id&lt;/div&gt;
                
                  &lt;div id="there"&gt;has an id&lt;/div&gt;
                  &lt;div&gt;nope&lt;/div&gt;
                &lt;script&gt;
                
                    $("div[id]").one("click", function(){
                      var idString = $(this).text() + " = " + $(this).attr("id");
                      $(this).text(idString);
                    });
                &lt;/script&gt;
                &lt;/body&gt;
                &lt;/html&gt;          %h4 Demo:
          .demo.code-demo
