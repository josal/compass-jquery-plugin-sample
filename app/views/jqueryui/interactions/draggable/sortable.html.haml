-@title = "#{t('txt.ui.name')} #{t('txt.ui.interactions.draggable.default')} - #{t('txt.ui.interactions.draggable.sortable')}"
-@jquery_partial = 'sortable.js.haml'
 
.demo
  %h3
    =@title
  %ul
    %li#draggable.ui-state-highlight
      Drag me down
  %ul#sortable
    %li.ui-state-default
      Item 1
    %li.ui-state-default
      Item 2
    %li.ui-state-default
      Item 3
    %li.ui-state-default
      Item 4
    %li.ui-state-default
      Item 5
/ End demo
.demo-description
  %p
    Draggables are built to interact seamlessly with
    %a{ :href => "#" }
      sortables
    \.
/ End demo-description
