-@title = "#{t('txt.ui.name')} #{t('txt.ui.widgets.datepicker.default')} - #{t('txt.ui.widgets.datepicker.other-months')}"
-@jquery_partial = 'other_months.js.haml'
      
.demo
  %h3
    =@title
  %p
    Date:
    %input#datepicker{ :type => "text" }
/ End demo
.demo-description
  %p
    The datepicker can show dates that come from other than the main month being displayed. These other dates can also be made selectable.
/ End demo-description
