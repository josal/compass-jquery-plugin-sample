-@title = "#{t('txt.ui.name')} #{t('txt.ui.widgets.selectmenu.name')} - #{t('txt.ui.widgets.selectmenu.custom-icons')}"

- content_for :content_head do
  = render(:partial => "custom_icons2.js.haml", :format => :js)

.demo
  %h3
    = raw @title
  %form{:action => "#"}
    %fieldset
      %label{:for => "filesB"}
        Select a File:
      %br
      %br
      %select#filesB.customicons{:name => "filesB"}
